{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": [
            "ESNext"
        ],
        "strict": false,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "https://*": [
                "./.lovable/types/https/*"
            ]
        }
    },
    "include": [
        "**/*.ts"
    ]
}